{
  "name": "opencloud-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --config jest.config.js",
    "test:fast": "jest --no-watchman --maxWorkers=4 --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:http": "jest services/api/__tests__/HttpUtil-test.ts --config jest.config.js",
    "test:e2e": "maestro test .maestro/flows/",
    "test:e2e:login": "maestro test .maestro/flows/01_login_screen.yaml",
    "test:e2e:theme": "maestro test .maestro/flows/02_theme_settings.yaml",
    "test:e2e:screenshots": "maestro test .maestro/flows/03_screenshot_login.yaml",
    "test:e2e:login:confirmation": "maestro test .maestro/flows/04_login_with_confirmation.yaml",
    "test:e2e:oidc": "maestro test .maestro/flows/05_oidc_login.yaml",
    "test:e2e:oidc:credentials": "node .maestro/run-with-credentials.js .maestro/flows/05_oidc_login.yaml",
    "test:e2e:oidc:token": "node .maestro/run-with-oidc-token.js .maestro/flows/06_oidc_token_login.yaml OpenCloudDesktop",
    "lint": "expo lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@remix-run/node": "^2.16.0",
    "@remix-run/server-runtime": "^2.16.0",
    "cookie": "^1.0.2",
    "expo": "~52.0.37",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "^3.1.0",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "metro": "^0.82.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.4",
    "npm-force-resolutions": "^0.0.10",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "resolutions": {
    "@babel/helpers": "7.26.10",
    "@babel/runtime": "7.26.10",
    "@babel/runtime-corejs3": "7.26.10",
    "cookie": "1.0.2"
  }
}
